{
  "rewrites": [
    { "source": "/coordenadas-esfericas", "destination": "/coordenadas-esfericas/index.html" },
    { "source": "/calculadora", "destination": "/calculadora/index.html" },
    { "source": "/parabolic-pets", "destination": "/parabolic-pets/index.html" },
    { "source": "/juegoinfantil", "destination": "/juegoinfantil/index.html" }
  ],
  "headers": [
    {
      "source": "/parabolic-pets/Build/(.*)",
      "headers": [
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/(.*).wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" }
      ]
    },
    {
      "source": "/(.*).(png|jpg|jpeg|webp|gif|svg|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*).(js|css|woff|woff2|ttf|otf)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
